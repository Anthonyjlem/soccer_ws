<?xml version="1.0"?>
<package format="2">
  <name>soccerbot</name>
  <version>0.0.0</version>
  <description>Main package to run soccerbot</description>

  <maintainer email="vuwij@me.com">Jason Wang</maintainer>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <!-- Hardware and Sensors-->
  <depend>soccer_hardware</depend>

  <!-- Communication and UI-->
  <depend>soccer_msgs</depend>
  <depend>soccer_team_communication</depend>
  <depend>soccer_monitor</depend>
  <depend>soccer_control_center</depend>

  <!-- Computer Vision and Localization-->
  <depend>amcl</depend>
  <depend>usb_cam</depend>
  <depend>map_server</depend>
  <depend>darknet_ros</depend>

  <!-- Control and Movement -->
  <depend>soccer_description</depend>
  <depend>soccer_matlab</depend>

  <!-- Simulation -->
  <depend>soccer_gazebo</depend>
  <depend>soccer_control</depend>
  <depend>controller_manager</depend>
  <depend>ros_control</depend>

  <!-- Omnibot-->
  <depend>omnibot_teleop</depend>
  <depend>omnibot_drive</depend>

</package>
